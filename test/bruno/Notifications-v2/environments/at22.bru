vars {
  host: https://platform.{{env}}.altinn.cloud
  Environment: at22
}

vars:secret [
  // URL for the token generator service
  TokenGeneratorBaseUrl,
  // JWT token that will be populated by the token generation request
  jwt,
  // Username for the token generator service
  TokenGeneratorUserName,
  // Password for the token generator service
  TokenGeneratorPassword
]
